# Queue mode
# If set to "sync", jobs will be processed synchronously without Redis
QUEUE_MODE=redis

# Upstash Redis REST credentials (required when QUEUE_MODE=redis)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Background removal API key (simulated)
BACKGROUND_REMOVAL_API_KEY=

# Daily job limit per user
DAILY_JOB_LIMIT=25

# Database path
DB_PATH=./data/db.sqlite

# Queue settings
QUEUE_ATTEMPTS=3
QUEUE_BACKOFF_MS=2000
